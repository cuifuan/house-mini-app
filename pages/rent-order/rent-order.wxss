page {
  font-size: 32rpx;
}

.content {
  height: 100%;
  background-color: #efefef;
  padding-top: 20rpx;
  box-sizing: border-box;
}

.first_card {
  height: 80rpx;
  background-color: #ffffff;
  border-radius: 8rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #f8f6f6;
  display: flex;
  align-items: center;
  line-height: 80rpx;
  padding-left: 20rpx;
}

.required {
  color: red;
  width: 10rpx;
  height: 80rpx;
  line-height: 80rpx;
  box-sizing: border-box;
}

.label {
  width: 230rpx;
  height: 80rpx;
  text-align: right;
  box-sizing: border-box;
  padding-right: 30rpx;
  font-weight: 350;
}

.input_cfa {
  width: 100%;
  height: 80rpx;
  color: #333333;
  padding-left: 8%;
  box-sizing: border-box;
}

.input_cfa_warn {
  border-bottom: 2rpx solid red;
  width: 100%;
  height: 80rpx;
  color: red;
  padding-left: 8%;
  box-sizing: border-box;
}

.placeholder_style {
  font-size: 32rpx;
  color: #ababab;
}

.placeholder_style_warn {
  color: red;
  font-size: 28rpx;
}

.picker_text {
  width: 100%;
  box-sizing: border-box;
  color: #333333;
  display: flex;
  padding-left: 50rpx;
  height: 80rpx;
  line-height: 80rpx;
}

.copy_right {
  width: 22rpx;
  height: 22rpx;
  border-width: 2rpx 2rpx 0 0;
  border-color: #ababab;
  border-style: solid;
  transform: rotate(45deg);
  margin-top: 28rpx;
}


/* uploader */

.uploader {
  background-color: white;
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
}

.uploader_header_title {
  /* width: 230rpx; */
  box-sizing: border-box;
  font-weight: 350;
  text-align: left;
  padding-top: 20rpx;
  padding-left: 40rpx;
  height: 80rpx;
}

.ui_uploader_cell {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  padding: 0 30rpx;
}
.ui_uploader_item{
  margin-right: 30rpx;
  margin-bottom: 30rpx;
  width: 200rpx;
  height: 200rpx;
  position: relative;
}
.ui_uploader_item_icon{
  position: absolute;
  right: -20rpx;
  top: -20rpx;
  background: #fff;
  border-radius: 50%;
}

.ui_uploader {
  margin-bottom: 25rpx;
  width: 200rpx;
  height: 200rpx;
  border: 2rpx solid #d9d9d9;
  box-sizing: border-box;
  position: relative;
}

.ui_uploader:before {
  content: " ";
  position: absolute;
  width: 4rpx;
  height: 79rpx;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  background-color: #d9d9d9;
}

.ui_uploader:after {
  content: " ";
  position: absolute;
  height: 4rpx;
  width: 79rpx;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  background-color: #d9d9d9;
}

.ui_uploader_item image {
  width: 100%;
  height: 100%;
}

.upload_explanation {
  color: #aeaeb2;
  margin-left: 70rpx;
}

.position {
  position: absolute;
  top: 100rpx;
  left: 250rpx;
  width: 400rpx;
  background: #ffffff;
  z-index: 1000;
  box-sizing: border-box;
  border: 2rpx solid #efefef;
  border-top: none;
  border-radius: 8rpx;
}

.dict_desc {
  border-bottom: 1rpx solid #efefef;
  text-align: center;
}